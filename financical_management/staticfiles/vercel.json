{
    "builds": [
    {
        "src": "financical_management/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    },
    {
        "src": "staticfiles/**/*",
        "use": "@vercel/static"
    }
],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "financical_management/wsgi.py"
        },
        { "src": "/static/(.*)", "dest": "/staticfiles/$1" }
    ]
}